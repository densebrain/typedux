{
  "name": "typedux",
  "version": "0.1.19",
  "description": "Slightly adjusted Redux (awesome by default) for TS with immutablejs built in",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "local-install": "npm i -g redux && npm link typeproject typelogger redux && npm i && npm link",
    "clean": "rm -Rf dist",
    "compile": "tsc --project tsconfig.json",
    "compile:watch": "tsc --project tsconfig.json --watch",
    "test": "npm run compile && ./node_modules/.bin/mocha --require dist/test/test-setup dist/test/TypeDux.spec.js",
    "publish:patch": "npm version patch && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/densebrain/typedux.git"
  },
  "keywords": [
    "redux",
    "typescript",
    "immutable"
  ],
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md"
  ],
  "author": "Jonathan Glanz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/densebrain/typedux/issues"
  },
  "homepage": "https://github.com/densebrain/typedux#readme",
  "peerDependencies": {
    "immutable": "^3.8.1",
    "redux": "^3.6.0",
    "reflect-metadata": "^0.1.3"
  },
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "bluebird": "^3.4.6",
    "browser-next-tick": "^1.1.0",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "source-map-support": "^0.5.1",
    "typelogger": "^1.0.24",
    "uuid": "^3.0.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/chai": "^4.1.1",
    "@types/immutable": "^3.8.4",
    "@types/mocha": "^2.2.32",
    "@types/node": "^9.3.0",
    "@types/redux": "^3.6.0",
    "@types/sinon": "^4.1.3",
    "babel-core": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-cli": "^2.0.0",
    "gulp-debug": "^3.2.0",
    "gulp-git": "^2.5.1",
    "gulp-mocha": "^5.0.0",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-typescript": "^4.0.0-alpha.1",
    "gulp-util": "^3.0.7",
    "merge2": "^1.0.2",
    "mocha": "^5.0.0",
    "mocha-junit-reporter": "^1.11.1",
    "run-sequence": "^2.2.1",
    "semver": "^5.1.0",
    "sinon": "^4.1.6",
    "source-map-loader": "^0.2.3",
    "typeproject": "^1.1.3",
