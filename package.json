{
  "name": "typedux",
  "version": "0.1.13",
  "description": "Slightly adjusted Redux (awesome by default) for TS with immutablejs built in",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "local-install": "npm i -g redux && npm link typeproject typelogger redux && npm i && npm link",
    "compile": "gulp compile",
    "test": "npm run compile && ./node_modules/.bin/mocha --require dist/test/test-setup dist/test/TypeDux.spec.js",
    "test:tag:publish": "gulp compile && npm test && gulp release-tag && npm publish",
    "compile-watch": "gulp compile-watch",
    "publish-patch": "npm version patch && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/densebrain/typedux.git"
  },
  "keywords": [
    "redux",
    "typescript",
    "immutable"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "author": "Jonathan Glanz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/densebrain/typedux/issues"
  },
  "homepage": "https://github.com/densebrain/typedux#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "bluebird": "^3.4.6",
    "browser-next-tick": "^1.1.0",
    "immutable": "^3.8.1",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "node-uuid": "^1.4.7",
    "redux": "^3.5.2",
    "reflect-metadata": "^0.1.3",
    "source-map-support": "^0.4.0",
    "typelogger": "^1.0.24"
  },
  "devDependencies": {
    "@types/bluebird": "^3.0.35",
    "@types/chai": "^3.4.34",
    "@types/immutable": "^3.8.4",
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.46",
    "@types/sinon": "^1.16.31",
    "babel-core": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.11.6",
    "babel-runtime": "^6.11.6",
    "chai": "^3.5.0",
    "del": "^2.2.0",
    "glob": "^7.0.3",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-cli": "^1.2.1",
    "gulp-debug": "^2.1.2",
    "gulp-git": "^1.7.1",
    "gulp-mocha": "^3.0.1",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-typescript": "^3.1.2",
    "gulp-util": "^3.0.7",
    "merge2": "^1.0.2",
    "mocha": "^3.1.2",
    "mocha-junit-reporter": "^1.11.1",
    "run-sequence": "^1.2.0",
    "semver": "^5.1.0",
    "sinon": "^1.17.4",
    "source-map-loader": "^0.1.5",
