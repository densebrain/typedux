{
  "name": "typedux",
  "version": "2.0.6",
  "description": "Slightly adjusted Redux (awesome by default) for TS with immutablejs built in",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "local-install": "npm i -g redux && npm link typeproject typelogger redux && npm i && npm link",
    "clean": "rm -Rf dist",
    "compile": "tsc --project tsconfig.json",
    "compile:watch": "tsc --project tsconfig.json --watch",
    "test": "npm run compile && ./node_modules/.bin/mocha --require dist/test/test-setup dist/test/TypeDux.spec.js",
    "publish:patch": "npm run compile && npm version patch && git push --tags && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/densebrain/typedux.git"
  },
  "keywords": [
    "redux",
    "typescript",
    "immutable"
  ],
  "files": [
    "src",
    "dist",
    "package.json",
    "README.md"
  ],
  "author": "Jonathan Glanz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/densebrain/typedux/issues"
  },
  "homepage": "https://github.com/densebrain/typedux#readme",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bluebird": "^3.5.1",
    "browser-next-tick": "^1.1.0",
    "lodash": "^4.17.10",
    "redux": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "shallowequal": "^1.1.0",
    "source-map-support": "^0.5.6",
    "typeguard": "0.0.12",
    "typelogger": "^1.0.29",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.22",
    "@types/chai": "^4.1.4",
    "@types/immutable": "^3.8.7",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.5.2",
    "@types/redux": "^3.6.0",
    "babel-core": "^6.26.3",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "chai": "^4.1.2",
    "del": "^3.0.0",
    "merge2": "^1.2.2",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.17.0",
    "run-sequence": "^2.2.1",
    "semver": "^5.5.0",
    "sinon": "^6.1.3",
    "source-map-loader": "^0.2.3",
    "typescript": "^2.9.2"
  }
}
