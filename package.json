  "description": "Slightly adjusted Redux (awesome by default) for TS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -Rf dist",
    "compile": "tsc --project tsconfig.json",
    "compile:watch": "tsc --project tsconfig.json --watch",
    "//test": "yarn compile && jest --require dist/test/test-setup dist/test/**/*.spec.js",
    "publish:patch": "npm run compile && npm version patch && git push --tags && npm publish"
