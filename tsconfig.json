{
	"compilerOptions": {
		"target": "es5",
		"lib": [
			"es6",
			"es7",
			"es2015",
			"es2016",
			"es2017",
			"dom"
		],
		"module": "commonjs",
		"moduleResolution": "node",
		"declaration": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"preserveConstEnums": true,
		"allowSyntheticDefaultImports": true,
		"sourceMap": true,
		"inlineSourceMap": true,
		"jsx": "react",
		"baseUrl": "./",
		"outDir": "dist",
		"typeRoots": [
			"./node_modules/@types",
			"./typings"
		]
		
	},
	"exclude": [
		"node_modules",
		"dist"
	]
}
